<%= form_with url: "/watches/#{@watch.id}", method: :patch, local: true do |form| %>
  <%= form.label "Model" %>
  <%= form.text_field :model %>
  <%= form.label "Bracelet" %>
  <%= form.text_field :country %>
  <%= form.label "Movement" %>
  <%= form.text_field :movement %>
  <%= form.label "Case Material" %>
  <%= form.text_field :case_material %>
   <%= form.label "Case Size" %>
  <%= form.text_field :case_size %> 
  <%= form.label "Crown Guard" %>
  <%= form.text_field :crown_guard %>

  <%= form.submit "Update Watch" %>
<% end %>