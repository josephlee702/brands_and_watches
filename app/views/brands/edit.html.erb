<%= form_with url: "/brands/#{@brand.id}", method: :patch, local: true do |form| %>
  <%= form.label "Name" %>
  <%= form.text_field :name %>
  <%= form.label "Country" %>
  <%= form.text_field :country %>
  <%= form.label "Year Founded" %>
  <%= form.text_field :year_founded %>

  <%= form.submit "Update Brand" %>
<% end %>